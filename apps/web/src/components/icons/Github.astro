---
import type { HTMLAttributes } from "astro/types";

interface Props extends HTMLAttributes<"svg"> {}
const { class: classNames, ...attrs } = Astro.props;
---

<svg role="img" viewBox="0 0 24 24" width="18" height="18" fill="currentColor" xmlns="http://www.w3.org/2000/svg" class={classNames} {...attrs}></svg>